{
  "name": "king-md",
  "description": "a Multi device Whatsapp bot",
  "keywords": [],
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "to run the bot",
      "required": true,
      "value": ""
    },
    "HEROKU_API_KEY": {
      "description": "For updating bot and setting vars remotely",
      "required": true,
      "value": ""
    },
    "HEROKU_APP_NAME": {
      "description": "Exact name which you gave at the top",
      "required": true,
      "value": ""
    },
    "WORKTYPE": {
      "description": "enter bot mode must be public/private",
      "required": true,
      "value": "private"
    },
    "BOT_NAME": {
      "description": "baasic bot configration",
      "required": true,
      "value": "KING-MD"
    },
    "PREFIX": {
      "description": "bot preifx  ., [.,!], false",
      "required": true,
      "value": "."
    },
    "OWNER_NUMBER": {
      "description": "audio metadata",
      "required": true,
      "value": "92xxxxxxxx"
    },
    "STICKER_DATA": {
      "description": "sticker data",
      "required": true,
      "value": "king;bot"
    },
    "AUTO_STATUS_VIEW": {
      "description": "automatic read status",
      "required": false,
      "value": "false"
    },
    "OWNER_NAME": {
      "description": "automatic reject call",
      "required": false,
      "value": "Naveed"
    },
    "WARN": {
      "description": "warn count",
      "required": false,
      "value": "4"
    },
    "SUDO": {
      "description": "secounds owners of bot",
      "required": true,
      "value": "null"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ]
}
